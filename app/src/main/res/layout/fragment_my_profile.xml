<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/extra_background_test"
    tools:context=".ui.MyProfile.MyProfileFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <TextView
            android:id="@+id/fullNameMP"
            android:layout_width="288dp"
            android:layout_height="60dp"
            android:layout_marginEnd="32dp"
            android:background="@color/white"
            android:clickable="true"
            android:elevation="4dp"
            android:padding="20dp"
            android:text="@string/my_profile_fName"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/textView4"
            app:layout_constraintTop_toBottomOf="@+id/textView4" />

        <TextView
            android:id="@+id/ageMP"
            android:layout_width="288dp"
            android:layout_height="60dp"
            android:layout_margin="5dp"
            android:background="@color/ic_launcher_background"
            android:clickable="true"
            android:elevation="4dp"
            android:padding="20dp"
            android:text="@string/my_profile_age"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/emailMP"
            app:layout_constraintStart_toStartOf="@+id/textView9"
            app:layout_constraintTop_toBottomOf="@+id/textView9" />

        <TextView
            android:id="@+id/highestQualificationMP"
            android:layout_width="288dp"
            android:layout_height="60dp"
            android:layout_margin="5dp"
            android:background="@color/ic_launcher_background"
            android:clickable="true"
            android:elevation="4dp"
            android:padding="20dp"
            android:text="@string/my_profile_qualification"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/ageMP"
            app:layout_constraintStart_toStartOf="@+id/textView11"
            app:layout_constraintTop_toBottomOf="@+id/textView11" />

        <TextView
            android:id="@+id/userNameMP"
            android:layout_width="288dp"
            android:layout_height="57dp"
            android:layout_margin="5dp"
            android:background="@color/ic_launcher_background"
            android:clickable="true"
            android:elevation="4dp"
            android:padding="20dp"
            android:text="@string/my_profile_uName"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/fullNameMP"
            app:layout_constraintStart_toStartOf="@+id/textView5"
            app:layout_constraintTop_toBottomOf="@+id/textView5" />

        <TextView
            android:id="@+id/emailMP"
            android:layout_width="288dp"
            android:layout_height="60dp"
            android:layout_margin="5dp"
            android:background="@color/ic_launcher_background"
            android:clickable="true"
            android:elevation="4dp"
            android:padding="20dp"
            android:text="@string/my_profile_email"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/userNameMP"
            app:layout_constraintStart_toStartOf="@+id/textView7"
            app:layout_constraintTop_toBottomOf="@+id/textView7" />

        <TextView
            android:id="@+id/skillsMP"
            android:layout_width="288dp"
            android:layout_height="60dp"
            android:layout_margin="5dp"
            android:background="@color/ic_launcher_background"
            android:clickable="true"
            android:elevation="4dp"
            android:padding="20dp"
            android:text="@string/my_profile_skills"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/highestQualificationMP"
            app:layout_constraintStart_toStartOf="@+id/textView16"
            app:layout_constraintTop_toBottomOf="@+id/textView16" />

        <TextView
            android:id="@+id/addressMP"
            android:layout_width="288dp"
            android:layout_height="60dp"
            android:layout_margin="5dp"
            android:background="@color/ic_launcher_background"
            android:clickable="true"
            android:elevation="4dp"
            android:padding="20dp"
            android:text="@string/my_profile_address"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/skillsMP"
            app:layout_constraintStart_toStartOf="@+id/textView17"
            app:layout_constraintTop_toBottomOf="@+id/textView17" />

        <TextView
            android:id="@+id/contactMP"
            android:layout_width="288dp"
            android:layout_height="60dp"
            android:layout_margin="5dp"
            android:layout_marginBottom="16dp"
            android:background="@color/ic_launcher_background"
            android:clickable="true"
            android:elevation="4dp"
            android:padding="20dp"
            android:text="@string/my_profile_contact"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/addressMP"
            app:layout_constraintStart_toStartOf="@+id/addressMP"
            app:layout_constraintTop_toBottomOf="@+id/textView18" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/imageview_profile_picture"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/ic_launcher_profile_picture_round"
            app:civ_border_width="4dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_launcher_profile_picture_round" />

        <TextView
            android:id="@+id/headlineMP"
            android:layout_width="122dp"
            android:layout_height="34dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="16dp"
            android:lines="4"
            android:text="Headline "
            android:textSize="24sp"
            android:textStyle="bold|italic"
            app:layout_constraintEnd_toStartOf="@+id/edit_profile_button"
            app:layout_constraintHorizontal_bias="0.326"
            app:layout_constraintStart_toEndOf="@+id/imageview_profile_picture"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/edit_profile_button"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginEnd="16dp"
            android:background="@android:drawable/ic_menu_edit"
            app:layout_constraintBottom_toBottomOf="@+id/headlineMP"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/headlineMP"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:text="@string/my_profile_fName"
            android:textColor="#0C0303"
            app:layout_constraintStart_toStartOf="@+id/imageview_profile_picture"
            app:layout_constraintTop_toBottomOf="@+id/imageview_profile_picture" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/my_profile_uName"
            android:textColor="#0E0A0A"
            app:layout_constraintStart_toStartOf="@+id/fullNameMP"
            app:layout_constraintTop_toBottomOf="@+id/fullNameMP" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/my_profile_email"
            android:textColor="#090202"
            app:layout_constraintStart_toStartOf="@+id/userNameMP"
            app:layout_constraintTop_toBottomOf="@+id/userNameMP" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/my_profile_age"
            android:textColor="#050101"
            app:layout_constraintStart_toStartOf="@+id/emailMP"
            app:layout_constraintTop_toBottomOf="@+id/emailMP" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/my_profile_qualification"
            android:textColor="#070404"
            app:layout_constraintStart_toStartOf="@+id/ageMP"
            app:layout_constraintTop_toBottomOf="@+id/ageMP" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="57dp"
            android:layout_height="16dp"
            android:layout_marginTop="16dp"
            android:text="@string/my_profile_skills"
            android:textColor="#050303"
            app:layout_constraintStart_toStartOf="@+id/highestQualificationMP"
            app:layout_constraintTop_toBottomOf="@+id/highestQualificationMP" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/my_profile_address"
            android:textColor="#020101"
            app:layout_constraintStart_toStartOf="@+id/skillsMP"
            app:layout_constraintTop_toBottomOf="@+id/skillsMP" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/my_profile_age"
            android:textColor="#0E0606"
            app:layout_constraintStart_toStartOf="@+id/addressMP"
            app:layout_constraintTop_toBottomOf="@+id/addressMP" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>
